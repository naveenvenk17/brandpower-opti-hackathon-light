[run]
source = src
branch = True
omit =
    */tests/*
    */venv/*
    */__pycache__/*
    src/main.py
    src/fastapi_app.py
    src/utils/web_utils.py
    src/utils/__init__.py
    src/models/__init__.py
    src/agent/master_agent.py

[report]
precision = 2
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod
    # Exclude template rendering and file downloads (UI infrastructure)
    return templates\.TemplateResponse
    return FileResponse
    # Exclude JSON persistence helpers (tested separately)
    _save_json
    _load_json

