[run]
source = src
branch = True
omit =
    */tests/*
    */venv/*
    */__pycache__/*
    src/utils/web_utils.py
    src/utils/__init__.py
    src/models/__init__.py
    src/agent/master_agent.py
    src/config.py
    src/logging_config.py

[report]
precision = 2
fail_under = 90
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod
    # Exclude template rendering and file downloads (UI infrastructure)
    return templates\.TemplateResponse
    return FileResponse
    # Exclude CLI entry point
    import argparse
    import uvicorn
    parser = argparse.ArgumentParser
    uvicorn.run

